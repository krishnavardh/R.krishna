#include<stdio.h>
int factorial(int);
void main()
{
int n,f;
printf("enter the number");
scanf("%d",&n);
f=factorial(n);
printf("factorial of the number is %d",f);
}
int factorial(int n)
{
int f;
if(n==1)
  return 1;
else
 f=n*factorial(n-1);
  return f;
  }
  
  
  
  
  
  OUTPUT:
  enter the number:5
  factorial of the number is 120
  
  
